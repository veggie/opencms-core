<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="galleryResources" type="org.opencms.ade.galleries.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel ui:field="m_parentPanel" addStyleNames="{galleryResources.previewDialogCss.previewDialog} {resources.generalCss.cornerAll} {resources.generalCss.textMedium}">
		<g:Label ui:field="m_title" styleName="{resources.dialogCss.caption}" addStyleNames="{galleryResources.previewDialogCss.previewTitle}" />
		<g:FlowPanel ui:field="m_previewHolder" addStyleNames="{galleryResources.previewDialogCss.previewHolder} {resources.generalCss.cornerAll}">
			<g:SimplePanel ui:field="m_previewPanel" addStyleNames="{galleryResources.previewDialogCss.previewPanel}" />
		</g:FlowPanel>
		<g:FlowPanel ui:field="m_tabsHolder" addStyleNames="{galleryResources.previewDialogCss.detailsHolder}"></g:FlowPanel>
		<g:FlowPanel ui:field="m_buttonBar" styleName="{galleryResources.previewDialogCss.previewButtonBar}">
			<cms:CmsPushButton ui:field="m_selectButton" addStyleNames="{galleryResources.previewDialogCss.previewButton}"></cms:CmsPushButton>
			<cms:CmsPushButton ui:field="m_closePreview" addStyleNames="{galleryResources.previewDialogCss.previewButton}"></cms:CmsPushButton>
		</g:FlowPanel>		
	</g:FlowPanel> 
</ui:UiBinder>