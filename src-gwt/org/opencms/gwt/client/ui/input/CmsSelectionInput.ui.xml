<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui"
	xmlns:cmi="urn:import:org.opencms.gwt.client.ui.input">
	<ui:with field="layout" type="org.opencms.gwt.client.ui.css.I_CmsInputLayoutBundle" />
	<ui:with field="main" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:HTMLPanel styleName="{layout.inputCss.selectionInput}" addStyleNames="{main.generalCss.cornerAll}">
		<cmi:CmsSimpleTextBox ui:field="m_textbox" />
		<cms:CmsPushButton ui:field="m_opener" addStyleNames="{layout.inputCss.opener}"/>
		<g:Label ui:field="m_fader" styleName="{layout.inputCss.fader}"  />
	</g:HTMLPanel>
</ui:UiBinder> 