<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:h="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:input="urn:import:org.opencms.gwt.client.ui.input">
	<ui:with field="style" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel>
		<input:CmsTextBox ui:field="m_box"></input:CmsTextBox>
		<g:FlowPanel ui:field="m_dateTimePanel">
			<h:DatePicker ui:field="m_picker"></h:DatePicker>
			<g:FlowPanel ui:field="m_timeField" addStyleNames="{style.dateBoxCss.dateTime}">
				<input:CmsTextBox ui:field="m_time" addStyleNames="{style.dateBoxCss.time}"></input:CmsTextBox>
				<input:CmsRadioButton ui:field="m_am" addStyleNames="{style.dateBoxCss.ampm}"></input:CmsRadioButton>
				<input:CmsRadioButton ui:field="m_pm" addStyleNames="{style.dateBoxCss.ampm}"></input:CmsRadioButton>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 
