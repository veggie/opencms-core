<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main .dialogInfo{
			margin:4px;
		}
		.main .container{
			border: 1px solid #aaaaaa;
			padding: 2px;
			border-radius: 4px;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.main}">
		<g:HTML ui:field="m_dialogInfo" styleName="{style.dialogInfo}" />
		<g:FlowPanel ui:field="m_container" styleName="{style.container}"/>
	</g:FlowPanel>
</ui:UiBinder> 